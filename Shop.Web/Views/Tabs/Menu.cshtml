
@model IEnumerable<string>

    @foreach (var tab in Model)
    {
        @Html.RouteLink(tab, new
            {
                controller = "Book",
                action = "List",
                category = tab,
            }, new
            {
                @class = "btn btn-default btn-lg",
                @style = "display:table-cell;"
                + (tab == ViewBag.SelectedCategory ? "background:#222222; color:white" : "")
            })
    }
